NAME = minishell

LIBFT = libft/libft.a

CFLAGS = -Wall -Wextra -Werror -I /usr/local/opt/readline/include
LINKING_FLAGS = -Wall -Wextra -Werror -lreadline -I /usr/local/opt/readline/include -L /usr/local/opt/readline/lib

EXPANSION_UTILS = ${addprefix expansions_utilities/, expanded_string_length_calculator.c get_local_env_var_value_from_name.c}
SOLITARY_QUOTES = ${addprefix solitary_quotes/, masking_solitary_quotes.c unmasking_solitary_quotes.c}
TOKENS_UTILS = ${addprefix tokens_utilities/, masking_whitespaces.c  other.c separate_special_tokens.c}
REDIRECTION_UTILS = ${addprefix redirection_utilities/, mask_heredoc_dollar_signs.c}
REDIRECTION_UTILS_ADD_REDIRECTION = ${addprefix redirection_utilities/add_redirection/, add_redirection.c helpers.c}
COMMANDS_UTILS =  ${addprefix commands_utilities/, commands_utilities.c  error_handling.c}
ENV_UTILS = ${addprefix env_utilities/, env_utilities.c env_lst_utilities.c}
UTILS = ${addprefix utilities/, clean_command_table.c free_command_table.c remove_literal_interpretation_indicators.c \
		 ${EXPANSION_UTILS} ${REDIRECTION_UTILS} ${REDIRECTION_UTILS_ADD_REDIRECTION} ${SOLITARY_QUOTES} ${TOKENS_UTILS} ${COMMANDS_UTILS} ${ENV_UTILS}}
PARSER = ${addprefix parser/, commands.c environment.c expansions.c heredoc.c parse.c redirections.c tokens.c \
		${UTILS}}
EXECUTE = ${addprefix execution/, ${addprefix source/, execute.c blt_ins.c env.c other_cmds.c export.c} \
		${addprefix utils/, utils.c env_utils.c child_care.c free.c errs.c cd_utils.c e_asterisk_utils.c traffic_light.c \
		plumber.c export_utils.c exp_unset_utils.c close_the_door.c patience.c env_addition.c}}

SIGNALS = ${addprefix signals/, ctrl_c.c ctrl_backslash.c}

PRE_PARSER = ${addprefix pre-parser/, pre-parser.c utilities.c}

SOURCE = ${addprefix source/, control_room.c main.c ${PARSER} ${EXECUTE} ${SIGNALS} ${PRE_PARSER}}

HEADER = ${addprefix source/, minishell.h}

OBJS = ${SOURCE:c=o}

all: ${NAME}

${NAME} : ${LIBFT} ${OBJS} ${HEADER} loop
	@${CC} ${OBJS} ${LIBFT} ${LINKING_FLAGS} -o ${NAME}

${LIBFT}:
	@make -C ./libft

clean:
	@${RM} ${OBJS}
	@make clean -C ./libft

fclean: clean
	@${RM} ${NAME}
	@make fclean -C ./libft

re: fclean all

loop:
	@for i in 1; do \
		$(MAKE) rick; \
	done
	@$(MAKE) endgame


